{
  "database": "./keys/database.json",
  "remotes": {
    "google": "./keys/google-remote.json",
    "local": "./keys/local.json"
  },
  "collections": {
    "userCollection": "users",
    "sessionCollection": "sessions",
    "volumesCollection": "volumes",
    "filesCollection": "files",
    "statsCollection": "storage-stats",
    "foreignKeys": "_foreignKeys"
  },
  "clientsFolder": "./clients/",
  "debug": false,
  "adminUser": "./keys/admin-user.json",
  "sessionSettings": {
    "sessionPath": "/",
    "sessionDomain": ".webinate.dev",
    "sessionPersistent": true,
    "sessionLifetime": 1800,
    "sessionLifetimeExtended": 172800,
    "secure": false
  },
  "websocket": {
    "host": "localhost",
    "port": 8020,
    "socketApiKey": "somepassword",
    "approvedSocketDomains": [
      "^localhost$"
    ]
  },
  "mail": {
    "type": "mailgun",
    "options": "./keys/mailgun.json"
  }
}